container_commands:
  01_create_superuser_for_django_admin:
    command: "python manage.py mysuperuser"
    leader_only: true
  02_migrate:
    command: "python manage.py migrate --noinput"
    leader_only: true
  03_static:
    command: "python manage.py collectstatic --noinput"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: SAMPLdjango.settings
